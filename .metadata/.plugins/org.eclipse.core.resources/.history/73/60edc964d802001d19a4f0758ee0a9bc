package classe;

public class DataTeste {

	public static void main(String[] args) {
		
		Data data1 = new Data();
		data1.dia = 01 ;
		data1.mes = 02;
		data1.ano = 1995;
		
		System.out.println(data1.obterDataFormatada());
		
		Data data2 = new Data();
		data2.dia = 16;
		data2.mes = 06;
		data2.ano = 2000;
		
		System.out.printf(data2.obterDataFormatada());
		
		var data3 = new Data();
		data3.dia = 23;
		data3.mes = 03;
		data3.ano = 1980;
		
		
		System.out.printf(data3.obterDataFormatada());

		

	}

}
